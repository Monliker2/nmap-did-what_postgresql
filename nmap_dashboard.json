          "rawQueryText": "SELECT\n  COUNT(DISTINCT p.port) AS \"Unique Ports\"\nFROM ports p\nJOIN hosts h ON p.host_id = h.id\nWHERE h.start_time >= $__from AND h.end_time < $__to AND p.state is 'open'\n",
          "rawSql": "WITH latest_time AS (\n  SELECT MAX(date_trunc('second', h.time)) AS max_time\n  FROM hosts h\n  WHERE $__timeFilter(h.time)\n)\nSELECT\n  date_trunc('second', h.time) AS \"time\",\n  COUNT(DISTINCT p.port) AS \"value\"\nFROM ports p\nJOIN hosts h ON p.host_id = h.id\nJOIN latest_time lt ON date_trunc('second', h.time) = lt.max_time\nWHERE p.state = 'open'\n  AND $__timeFilter(h.time)\nGROUP BY 1\nORDER BY 1;\n",
